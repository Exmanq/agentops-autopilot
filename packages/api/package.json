{
  "name": "@agentops-autopilot/api",
  "version": "0.1.0",
  "description": "Fastify API for agentops-autopilot",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-esm src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "demo": "node dist/demo.js",
    "db:migrate": "node src/migrations/mock.js"
  },
  "dependencies": {
    "@agentops-autopilot/core": "workspace:*",
    "@agentops-autopilot/tools": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.4.1",
    "pg": "^8.11.5",
    "pino": "^9.2.0",
    "prom-client": "^15.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
